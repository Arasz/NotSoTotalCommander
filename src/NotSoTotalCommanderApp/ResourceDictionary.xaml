<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:res="clr-namespace:NotSoTotalCommanderApp.Culture"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    d1p1:Ignorable="d"
                    xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006">
    <vm:ViewModelLocator x:Key="Locator" d:IsDataSource="True" xmlns:vm="clr-namespace:NotSoTotalCommanderApp.ViewModel" />
    <!--ObjectDataProvider allows binding to object returned from method -->
    <ObjectDataProvider x:Key="Resources" ObjectType="{x:Type res:CultureResources}" MethodName="GetResourceInstance" />
</ResourceDictionary>